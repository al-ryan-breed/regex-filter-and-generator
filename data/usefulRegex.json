{
    "usefulRegex": [{
        "name": "Email",
        "expression": "^.+@.+$",
        "flags": "g"
    }, {
        "name": "Hex Color Value",
        "expression": "^#?([a-fA-F0-9]{6}|[a-fA-F0-9]{3})$",
        "flags": "g"
    }, {
        "name": "IPv4",
        "expression": "^(?:(?:25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\\.){3}(?:25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)$",
        "flags": "g"
    }, {
        "name": "IPv6",
        "expression": "^(([0-9a-fA-F]{1,4}:){7,7}[0-9a-fA-F]{1,4}|([0-9a-fA-F]{1,4}:){1,7}:|([0-9a-fA-F]{1,4}:){1,6}:[0-9a-fA-F]{1,4}|([0-9a-fA-F]{1,4}:){1,5}(:[0-9a-fA-F]{1,4}){1,2}|([0-9a-fA-F]{1,4}:){1,4}(:[0-9a-fA-F]{1,4}){1,3}|([0-9a-fA-F]{1,4}:){1,3}(:[0-9a-fA-F]{1,4}){1,4}|([0-9a-fA-F]{1,4}:){1,2}(:[0-9a-fA-F]{1,4}){1,5}|[0-9a-fA-F]{1,4}:((:[0-9a-fA-F]{1,4}){1,6})|:((:[0-9a-fA-F]{1,4}){1,7}|:)|fe80:(:[0-9a-fA-F]{0,4}){0,4}%[0-9a-zA-Z]{1,}|::(ffff(:0{1,4}){0,1}:){0,1}((25[0-5]|(2[0-4]|1{0,1}[0-9]){0,1}[0-9])\\.){3,3}(25[0-5]|(2[0-4]|1{0,1}[0-9]){0,1}[0-9])|([0-9a-fA-F]{1,4}:){1,4}:((25[0-5]|(2[0-4]|1{0,1}[0-9]){0,1}[0-9])\\.){3,3}(25[0-5]|(2[0-4]|1{0,1}[0-9]){0,1}[0-9]))$",
        "flags": "g"
    }, {
        "name": "URL",
        "expression": "^(file|gopher|news|nntp|telnet|https?|ftps?|sftp):\/\/([a-z0-9-]+\\.)+[a-z0-9]{2,4}.*$",
        "flags": "g"
    }, {
        "name": "Positive Number",
        "expression": "^\\d*\\.?\\d+$",
        "flags": "g"
    }, {
        "name": "Negative Number",
        "expression": "^-\\d*\\.?\\d+$",
        "flags": "g"
    }, {
        "name": "Number",
        "expression": "^-?\\d*\\.?\\d+$",
        "flags": "g"
    }, {
        "name": "UserName",
        "expression": "^[a-z0-9_-]{3,16}$",
        "flags": "g"
    }, {
        "name": "Password",
        "expression": "^[a-z0-9_-]{6,18}$",
        "flags": "g"
    }, {
        "name": "HTML Tag",
        "expression": "^<([a-z]+)([^<]+)*(?:>(.*)<\/\\1>|\\s+\/>)$",
        "flags": "g"
    }, {
        "name": "Number&Alphabet",
        "expression": "^[a-zA-Z0-9]+$",
        "flags": "g"
    }, {
        "name": "Alphabet",
        "expression": "^[a-zA-Z]+$",
        "flags": "g"
    }]
}
